    "prettier": "^3.5.2",
    "storybook": "^8.5.8",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
