{
  "name": "stock-analytics",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check-types": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit",
    "dev": "vite --mode development",
    "build": "npm run check-types && vite build --mode production",
    "build:dev": "npm run check-types && vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "test": "vitest run",
    "test-watch": "vitest watch",
    "coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --exit-once-uploaded --exit-zero-on-changes --only-changed"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.17.0",
    "@fast-check/vitest": "^0.1.6",
    "@solidjs/testing-library": "^0.8.10",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/builder-vite": "^8.6.4",
    "@storybook/html-vite": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@tailwindcss/vite": "^4.0.12",
    "@types/browser-lang": "^0.1.2",
    "@types/humps": "^2.0.6",
    "@types/node": "^22.13.10",
    "@types/string-format": "^2.0.3",
    "@vitest/coverage-v8": "^3.0.6",
    "axios-mock-adapter": "^2.1.0",
    "chromatic": "^11.25.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^15.14.0",
    "import": "^0.0.6",
    "jsdom": "^26.0.0",
    "msw": "^2.7.3",
    "path": "^0.12.7",
    "prettier": "^3.5.2",
    "storybook": "^8.5.8",
    "storybook-solidjs": "^1.0.0-beta.6",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.2.1",
    "vite-plugin-serve-static": "^1.1.0",
    "vite-plugin-solid": "^2.11.6",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "@solid-primitives/i18n": "^2.2.0",
    "@solidjs/router": "^0.15.3",
    "browser-lang": "^0.2.1",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.4",
    "humps": "^2.0.1",
    "ky": "^1.7.5",
    "neverthrow": "^8.2.0",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.5",
    "solid-meta": "^0.28.1",
    "solid-utils": "^0.8.1",
    "string-format": "^2.0.0",
    "tailwindcss": "^4.0.12",
    "zod": "^3.24.2"
  }
}
